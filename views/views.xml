  <act_window
  id="action_all_books"
  name="Libros"
  res_model="libros.libro"
  view_mode="tree"/>

  <data>
    <record id="view_all_books_tree" model="ir.ui.view">
      <field name="name">Libros</field>
      <field name="model">libros.libro</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
